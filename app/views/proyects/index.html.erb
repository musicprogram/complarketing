<%- model_class = Proyect -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:campaignName_id) %></th>
      <th><%= model_class.human_attribute_name(:brandName_id) %></th>
      <th><%= model_class.human_attribute_name(:country_id) %></th>
      <th><%= model_class.human_attribute_name(:nameEmployee_id) %></th>
      <th><%= model_class.human_attribute_name(:nameActivity_id) %></th>
      <th><%= model_class.human_attribute_name(:typeActivity_id) %></th>
      <th><%= model_class.human_attribute_name(:note_id) %></th>
      <th><%= model_class.human_attribute_name(:dateActivity_id) %></th>
      <th><%= model_class.human_attribute_name(:endDateActivity_id) %></th>
      <th><%= model_class.human_attribute_name(:totalActivity_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @proyects.each do |proyect| %>
      <tr>
        <td><%= link_to proyect.id, proyect_path(proyect) %></td>
        <td><%= proyect.campaignName_id %></td>
        <td><%= proyect.brandName_id %></td>
        <td><%= proyect.country_id %></td>
        <td><%= proyect.nameEmployee_id %></td>
        <td><%= proyect.nameActivity_id %></td>
        <td><%= proyect.typeActivity_id %></td>
        <td><%= proyect.note_id %></td>
        <td><%= proyect.dateActivity_id %></td>
        <td><%= proyect.endDateActivity_id %></td>
        <td><%= proyect.totalActivity_id %></td>
        <td><%=l proyect.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_proyect_path(proyect), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      proyect_path(proyect),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_proyect_path,
            :class => 'btn btn-primary' %>
