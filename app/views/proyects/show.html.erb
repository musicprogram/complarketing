<%- model_class = Proyect -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:campaignName_id) %>:</strong></dt>
  <dd><%= @proyect.campaignName_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:brandName_id) %>:</strong></dt>
  <dd><%= @proyect.brandName_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:country_id) %>:</strong></dt>
  <dd><%= @proyect.country_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nameEmployee_id) %>:</strong></dt>
  <dd><%= @proyect.nameEmployee_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nameActivity_id) %>:</strong></dt>
  <dd><%= @proyect.nameActivity_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:typeActivity_id) %>:</strong></dt>
  <dd><%= @proyect.typeActivity_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:note_id) %>:</strong></dt>
  <dd><%= @proyect.note_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dateActivity_id) %>:</strong></dt>
  <dd><%= @proyect.dateActivity_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:endDateActivity_id) %>:</strong></dt>
  <dd><%= @proyect.endDateActivity_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:totalActivity_id) %>:</strong></dt>
  <dd><%= @proyect.totalActivity_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              proyects_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_proyect_path(@proyect), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              proyect_path(@proyect),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
